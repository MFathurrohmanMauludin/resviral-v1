{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "no-console": 0,
    "no-underscore-dangle": 0,
    "no-restricted-globals": 0,
    "linebreak-style": 0,
    "indent": 0,
    "no-plusplus": 0,
    "no-use-before-define": 0,
    "max-len": 0,
    "import/extensions": 0,
    "class-methods-use-this": 0
  }
}
